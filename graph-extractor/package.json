{
  "name": "graph-extractor",
  "version": "0.1.0",
  "description": "Graph-based code extractor for RAG system",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
    "start": "bun build/index.js",
    "dev": "ts-node-esm src/index.ts",
